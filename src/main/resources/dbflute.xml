	<component name="behaviorSelector" class="com.todostack.dbflute.allcommon.ImplementedBehaviorSelector"/>
	<component name="commonColumnAutoSetupper" class="com.todostack.dbflute.allcommon.ImplementedCommonColumnAutoSetupper"/>

	<!-- The components of Behavior. -->
	<component name="memberBhv" class="com.todostack.dbflute.exbhv.MemberBhv"/>
	<component name="memberSecurityBhv" class="com.todostack.dbflute.exbhv.MemberSecurityBhv"/>
	<component name="taskBhv" class="com.todostack.dbflute.exbhv.TaskBhv"/>
